// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Admin {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Salon {
  id        Int      @id @default(autoincrement())
  name      String
  owner     String
  email     String   @unique
  address   String
  type      String
  paid      Boolean  @default(false)
  createdAt DateTime @default(now())
}
